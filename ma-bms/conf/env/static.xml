<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE env PUBLIC "-//GOYY//DTD ENV 1.0//EN" "http://goyy.org/dtd/env-1.0.dtd">
<configuration>
	<environments default="development">
		<environment id="development">
			<asset name="minapp">
				<enable>true</enable>
				<dir>/assets</dir>
				<url>/assets</url>
			</asset>
			<static name="minapp">
				<enable>true</enable>
				<dir>static</dir>
				<url>/statics</url>
			</static>
			<developer name="minapp">
				<enable>true</enable>
				<dir>/assets/madev</dir>
				<url>/madev</url>
			</developer>
			<operation name="minapp">
				<enable>true</enable>
				<dir>/assets/maopr</dir>
				<url>/maopr</url>
			</operation>
		</environment>
		<environment id="test">
			<asset name="minapp">
				<enable>false</enable>
				<dir>/app/assets</dir>
				<url>http://bl.hn.yixin.com</url>
			</asset>
			<static name="minapp">
				<enable>false</enable>
				<dir>/app/assets/mabms</dir>
				<url>http://bl.hn.yixin.com/mabms</url>
			</static>
			<developer name="minapp">
				<enable>false</enable>
				<dir>/app/assets/madev</dir>
				<url>http://bl.hn.yixin.com/madev</url>
			</developer>
			<operation name="minapp">
				<enable>false</enable>
				<dir>/app/assets/maopr</dir>
				<url>http://bl.hn.yixin.com/maopr</url>
			</operation>
		</environment>
		<environment id="production">
			<asset name="minapp">
				<enable>false</enable>
				<dir>/assets</dir>
				<url>http://assets.yixinhuinong.com</url>
			</asset>
			<static name="minapp">
				<enable>false</enable>
				<dir>/assets/mabms</dir>
				<url>http://assets.yixinhuinong.com/mabms</url>
			</static>
			<developer name="minapp">
				<enable>false</enable>
				<dir>/assets/madev</dir>
				<url>http://assets.yixinhuinong.com/madev</url>
			</developer>
			<operation name="minapp">
				<enable>false</enable>
				<dir>/assets/maopr</dir>
				<url>http://assets.yixinhuinong.com/maopr</url>
			</operation>
		</environment>
	</environments>
</configuration>
