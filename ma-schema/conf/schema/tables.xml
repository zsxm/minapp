<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE xtab PUBLIC "-//GOYY//DTD XTAB 1.0//EN" "http://goyy.org/dtd/xtab-1.0.dtd">
<configuration>
	<tables>
		<table id="pk" name="主键父表" comment="只包含主键的父表">
			<column extends="id"/>
		</table>
		<table id="sys" name="系统父表" extends="pk" comment="包含系统字段的父表">
			<column extends="memo"/>
			<column extends="creates"/>
			<column extends="creater"/>
			<column extends="created"/>
			<column extends="modifier"/>
			<column extends="modified"/>
			<column extends="version"/>
			<column extends="deletion"/>
			<column extends="artifical"/>
			<column extends="history"/>
		</table>
		<table id="tree" name="树结构父表" extends="sys" comment="包含树结构字段的父表">
			<column extends="code"/>
			<column extends="name"/>
			<column extends="fullname"/>
			<column extends="genre"/>
			<column extends="ordinal"/>
			<column extends="parent_id"/>
			<column extends="parent_ids"/>
			<column extends="parent_codes"/>
			<column extends="parent_names"/>
			<column extends="leaf"/>
			<column extends="grade"/>
		</table>
		
		<!--自定义父表-->
		<table id="applying" name="申请父表" extends="sys" comment="申请父表">
			<column extends="area_id" name="所在区域" comment="所在区域"/>
			<column extends="name" name="您的姓名" comment="申请人姓名"/>
			<column extends="tel" name="联系电话" comment="申请人联系电话"/>
			<column extends="source" name="申请来源" comment="申请来源 110:电脑版首页 120:电脑版二级页 210:触屏版首页 220:触屏版二级页" default="110"/>
			<column extends="mark" name="处理标记" comment="处理标记 0:未处理 1:已处理" default="0"/>
			<column extends="role_id"/>
		</table>
	</tables>
</configuration>
